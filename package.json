{
	"name": "klabin-sso-security-report",
	"version": "1.0.0",
	"description": "Klabin SSO Security Report",
	"devDependencies": {
		"babel-cli": "6.26.0",
		"babel-loader": "7.1.5",
		"babel-preset-env": "1.7.0",
		"babel-preset-react": "6.24.1",
		"copy-webpack-plugin": "4.6.0",
		"liferay-npm-build-support": "^2.16.0",
		"liferay-npm-bundler": "^2.16.0",
		"webpack": "4.29.6",
		"webpack-cli": "3.3.0",
		"webpack-dev-server": "3.2.1"
	},
	"dependencies": {
		"@apollo/react-hooks": "^3.1.3",
		"apollo-boost": "^0.4.4",
		"apollo-cache-inmemory": "^1.6.3",
		"apollo-client": "^2.6.4",
		"apollo-link-context": "^1.0.19",
		"apollo-link-http": "^1.5.16",
		"react-apollo": "^3.1.3",
		"dotenv": "^6.2.0",
		"graphql": "^14.5.8",
		"graphql-tag": "^2.10.1",
		"react": "16.8.6",
		"react-dom": "16.8.6"
	},
	"scripts": {
		"build": "babel --source-maps -d build src && npm run copy-assets && liferay-npm-bundler",
		"copy-assets": "lnbs-copy-assets",
		"translate": "lnbs-translate",
		"deploy": "npm run build && lnbs-deploy",
		"start": "lnbs-start"
	},
	"portlet": {
		"com.liferay.portlet.display-category": "category.klabin",
		"com.liferay.portlet.header-portlet-css": "/css/styles.css",
		"com.liferay.portlet.instanceable": true,
		"javax.portlet.name": "klabinssosecurityreport",
		"javax.portlet.security-role-ref": "power-user,user",
		"javax.portlet.resource-bundle": "content.Language"
	},
	"main": "index.js"
